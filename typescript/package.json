{
  "name": "unified-webhook-router",
  "version": "1.0.0",
  "type": "module",
  "description": "A unified webhook router for handling webhooks from multiple providers (Stripe, GitHub, Slack, etc.)",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.cjs.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "rollup -c",
    "build:tsc": "tsc",
    "test": "jest",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "webhook",
    "stripe",
    "github",
    "slack",
    "twilio",
    "square",
    "router",
    "verification"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-typescript": "^12.1.3",
    "@types/jest": "^29.0.0",
    "@types/node": "^20.0.0",
    "jest": "^29.0.0",
    "rollup": "^4.44.0",
    "rollup-plugin-dts": "^6.2.1",
    "ts-jest": "^29.0.0",
    "tslib": "^2.8.1",
    "typescript": "^5.0.0"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lanemc/webhook-router.git"
  },
  "bugs": {
    "url": "https://github.com/lanemc/webhook-router/issues"
  },
  "homepage": "https://github.com/lanemc/webhook-router#readme",
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
